{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es6",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "baseUrl": "types",
    "typeRoots": ["types"],
    "forceConsistentCasingInFileNames": true,
    "rootDir": "types",
    "types": [],
    "sourceMap": true,
    "declaration": true
  }
}
